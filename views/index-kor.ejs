      <div data-anchor="accueil" id="section-home" class="section">
        <% if (is_desktop) { %>
          <div class="background-slider">
            <% for(var z=0; z<projects.length; z++) { %>
              <% if (projects[z].showcase) { %>
                <% if (projects[z].showcase_cover) { %>
                  <% if (projects[z].showcase_type == "video/mp4") { %>
                    <div class="background-slide" data-name="<%= projects[z].name %>-<%= projects[z].id %>" data-id="<%= projects[z].id %>"> <video class="background-video" loop muted data-autoplay> <source src="/uploads/projects/covers/<%= projects[z].showcase_cover %>" type="video/mp4"> </video> </div>
