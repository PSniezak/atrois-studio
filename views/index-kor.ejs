      <% for(var j=0; j<projects.length; j++) { %>
        <div id="<%= projects[j].id %>" class="unique-slider">
          <div class="slide presentation-slide">
            <div class="left-cover"></div>
            <div class="right-cover"></div>
            <div class="presentation-container">
              <div class="left"> <span class="client"><%= projects[j].client_kor %></span> <br> <span class="season"><%= projects[j].season %></span> <br>
                <p class="categories">
                  <%= projects[j].categories_kor %>
                </p>
                <div class="url-container">
                  <a class="url" target="_blank" href="http://<%= projects[j].url %>">
                    <%= projects[j].url %>
                  </a>
                </div>
              </div>
              <div class="right"> <span class="name"><%= projects[j].name_kor %></span> <br>
                <div class="overflower-container">
                  <div class="overflower">
                    <p class="description">
                      <%- projects[j].description_kor %>
                    </p>
                  </div> <a class="toggle-more">이 프로젝트의 자세한 정보</a> </div>
              </div>
            </div>
          </div>
          <% if (is_desktop && projects[j].iframe_active) { %>
            <div class="slide iframe-slide">
              <div class="left-cover"></div>
              <div class="right-cover"></div>
              <div class="website-container"> <iframe src="https://<%= projects[j].url %>" width="" height=""></iframe> </div>
            </div>
            <% } %>
        </div>
