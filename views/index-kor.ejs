                      <div class="background-slide" data-lazy="/uploads/projects/covers/<%= projects[z].showcase_cover %>" style="background-image: url(/uploads/projects/covers/<%= projects[z].showcase_cover %>)" data-name="<%= projects[z].name %>-<%= projects[z].id %>"
                        data-id="<%= projects[z].id %>"></div>
                      <% } %>
                        <% } %>
                          <% } %>
                            <% } %>
