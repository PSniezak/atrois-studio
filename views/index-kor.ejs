      <script async type="text/javascript" src="/javascripts/main.js"></script>
      <script type="text/javascript">
        $(window).on('load', function() {
          var isMobile = false;
          if (window.innerWidth < 768) {
            isMobile = true;
          }
          setTimeout(function() {
            $('#loader').fadeOut();
            $('#main').animate({
              opacity: 1
            }, 1000, function() {
              if (isMobile) {
                $('#section-home .background-slider-mobile').slick('slickPlay');
              } else {
                $('#section-home .background-slider').slick('slickPlay');
              }
              if (sessionStorage.getItem('project')) {
                $('.project-link[data-id="' + sessionStorage.getItem('project') + '"]').click();
                sessionStorage.removeItem('project');
              }
            });
          }, 2000);
        });
      </script>
