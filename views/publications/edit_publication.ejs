<% for(var i=0; i<data.length; i++) { %>
	<form action="/publications/edit/<%= data[i].id %>" method="POST">
		<label for="season">Saison</label>
		<input type="text" name="season" id="season" value="<%= data[i].season %>">

		<label for="season_en">Saison EN</label>
		<input type="text" name="season_en" id="season_en" value="<%= data[i].season_en %>">

		<label for="season_jp">Saison JP</label>
		<input type="text" name="season_jp" id="season_jp" value="<%= data[i].season_jp %>">

		<label for="season_kor">Saison</label>
		<input type="text" name="season_kor" id="season_kor" value="<%= data[i].season_kor %>">


		<label for="title">Titre</label>
		<input type="text" name="title" id="title" value="<%= data[i].title %>">

		<label for="title_en">Titre EN</label>
		<input type="text" name="title_en" id="title_en" value="<%= data[i].title_en %>">

		<label for="title_jp">Titre JP</label>
		<input type="text" name="title_jp" id="title_jp" value="<%= data[i].title_jp %>">

		<label for="title_kor">Titre KOR</label>
		<input type="text" name="title_kor" id="title_kor" value="<%= data[i].title_kor %>">


		<label for="client">Client</label>
		<input type="text" name="client" id="client" value="<%= data[i].client %>">

		<label for="client_en">Client EN</label>
		<input type="text" name="client_en" id="client_en" value="<%= data[i].client_en %>">

		<label for="client_jp">Client JP</label>
		<input type="text" name="client_jp" id="client_jp" value="<%= data[i].client_jp %>">

		<label for="client_kor">Client KOR</label>
		<input type="text" name="client_kor" id="client_kor" value="<%= data[i].client_kor %>">


		<label for="url">Lien</label>
		<input type="text" name="url" id="url" value="<%= data[i].url %>">

		<label for="award">Award</label>
		<input type="checkbox" name="award" id="award" <% if (data[i].award == true) { -%>checked<% } -%>>

		<label for="place">Ordre</label>
		<input type="text" name="place" id="place" value="<%= data[i].place %>">

		<input type="submit" value="Ã‰diter cette publication">
	</form>
<% } %>
