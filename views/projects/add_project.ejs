<form action="/projects/add" method="POST" enctype="multipart/form-data">
	<label for="year">Année</label>
	<input type="text" name="year" id="year">


	<label for="client">Client</label>
	<input type="text" name="client" id="client">

	<label for="client_en">Client EN</label>
	<input type="text" name="client_en" id="client_en">

	<label for="client_jp">Client JP</label>
	<input type="text" name="client_jp" id="client_jp">

	<label for="client_kor">Client KOR</label>
	<input type="text" name="client_kor" id="client_kor">


	<label for="name">Nom</label>
	<input type="text" name="name" id="name">

	<label for="name_en">Nom EN</label>
	<input type="text" name="name_en" id="name_en">

	<label for="name_jp">Nom JP</label>
	<input type="text" name="name_jp" id="name_jp">

	<label for="name_kor">Nom KOR</label>
	<input type="text" name="name_kor" id="name_kor">


	<label for="categories">Catégories</label>
	<input type="text" name="categories" id="categories">

	<label for="categories_en">Catégories EN</label>
	<input type="text" name="categories_en" id="categories_en">

	<label for="categories_jp">Catégories JP</label>
	<input type="text" name="categories_jp" id="categories_jp">

	<label for="categories_kor">Catégories KOR</label>
	<input type="text" name="categories_kor" id="categories_kor">


	<label for="season">Saison</label>
	<input type="text" name="season" id="season">

	<label for="season_en">Saison EN</label>
	<input type="text" name="season_en" id="season_en">

	<label for="season_jp">Saison JP</label>
	<input type="text" name="season_jp" id="season_jp">

	<label for="season_kor">Saison KOR</label>
	<input type="text" name="season_kor" id="season_kor">


	<label for="url">Lien</label>
	<input type="text" name="url" id="url">

	<label for="description">Description</label>
	<textarea type="text" name="description" id="description" rows="8" cols="80"></textarea>

	<label for="description_en">Description EN</label>
	<textarea type="text" name="description_en" id="description_en" rows="8" cols="80"></textarea>

	<label for="description_jp">Description JP</label>
	<textarea type="text" name="description_jp" id="description_jp" rows="8" cols="80"></textarea>

	<label for="description_kor">Description KOR</label>
	<textarea type="text" name="description_kor" id="description_kor" rows="8" cols="80"></textarea>

	<label for="place">Ordre</label>
	<input type="text" name="place" id="place">

	<label for="cover">Cover</label>
	<input type="file" name="cover" id="cover" value="">

	<input type="submit" value="Ajouter ce projet">
</form>
