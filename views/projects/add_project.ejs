<form action="/projects/add" method="POST" enctype="multipart/form-data">
	<label for="year">Année</label>
	<input type="text" name="year" id="year">

	<label for="client">Client</label>
	<input type="text" name="client" id="client">

	<label for="name">Nom</label>
	<input type="text" name="name" id="name">

	<label for="categories">Catégories</label>
	<input type="text" name="categories" id="categories">

	<label for="season">Saison</label>
	<input type="text" name="season" id="season">

	<label for="url">Lien</label>
	<input type="text" name="url" id="url">

	<label for="description">Description</label>
	<textarea type="text" name="description" id="description" rows="8" cols="80"></textarea>

	<label for="place">Ordre</label>
	<input type="text" name="place" id="place">

	<label for="cover">Cover</label>
	<input type="file" name="cover" id="cover" value="">

	<input type="submit" value="Ajouter ce projet">
</form>
